{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"outDir": "./dist",
		"rootDir": "./",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["es2019"],
		"types": ["node"]
	},
	"include": ["**/*.ts"],
	"exclude": ["node_modules"]
} 